        "import pandas as pd\n",
        "import numpy as np\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.feature_extraction.text import CountVectorizer, TfidfVectorizer\n",
        "from sklearn.naive_bayes import MultinomialNB\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "from sklearn.svm import LinearSVC\n",
        "from sklearn.pipeline import Pipeline\n",
        "from sklearn.metrics import accuracy_score, f1_score, precision_score, recall_score, confusion_matrix, classification_report\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
       
        "X_train, X_val, y_train, y_val = train_test_split(\n",
        "    train_df[\"text\"], train_df[\"target\"], test_size=0.2, random_state=42\n",
        ")\n",
        "\n",
      
        "vectorizers = {\n",
        "    \"BoW\": CountVectorizer(max_features=10000),\n",
        "    \"TF-IDF\": TfidfVectorizer(max_features=10000)\n",
        "}\n",
        "\n",
        "models = {\n",
        "    \"NaiveBayes\": MultinomialNB(),\n",
        "    \"LogReg\": LogisticRegression(max_iter=200, n_jobs=-1),\n",
        "    \"LinearSVM\": LinearSVC()\n",
        "}\n",
        "\n",
        "results = []\n",
        "\n",
        "for vec_name, vectorizer in vectorizers.items():\n",
        "    for model_name, model in models.items():\n",
        "        # NB only works with BoW/TF-IDF\n",
        "        if model_name == \"NaiveBayes\" and vec_name not in [\"BoW\", \"TF-IDF\"]:\n",
        "            continue\n",
        "\n",
        "        pipe = Pipeline([\n",
        "            (\"vec\", vectorizer),\n",
        "            (\"clf\", model)\n",
        "        ])\n",
        "\n",
        "        pipe.fit(X_train, y_train)\n",
        "        y_pred = pipe.predict(X_val)\n",
        "\n",
        "        acc = accuracy_score(y_val, y_pred)\n",
        "        f1  = f1_score(y_val, y_pred, pos_label=\"positive\")\n",
        "\n",
        "        results.append({\n",
        "            \"Vectorizer\": vec_name,\n",
        "            \"Model\": model_name,\n",
        "            \"Accuracy\": acc,\n",
        "            \"F1\": f1\n",
        "        })\n",
        "\n",
        "        print(f\"=== {vec_name} + {model_name} ===\")\n",
        "        print(classification_report(y_val, y_pred))\n",
        "        print(\"\\n\")\n",
        "\n",
      
        "results_df = pd.DataFrame(results)\n",
        "print(\"📊 Comparison Table:\")\n",
        "print(results_df)\n",
        "\n",
        "# 🟢 Confusion Matrix لأفضل موديل\n",
        "best = results_df.sort_values(by=\"F1\", ascending=False).iloc[0]\n",
        "print(\"\\nBest Model:\", best[\"Vectorizer\"], \"+\", best[\"Model\"])\n",
        "\n",
     
        "best_vec = vectorizers[best[\"Vectorizer\"]]\n",
        "best_model = models[best[\"Model\"]]\n",
        "best_pipe = Pipeline([(\"vec\", best_vec), (\"clf\", best_model)])\n",
        "best_pipe.fit(X_train, y_train)\n",
        "y_pred = best_pipe.predict(X_val)\n",
        "\n",
        "cm = confusion_matrix(y_val, y_pred, labels=[\"negative\", \"positive\"])\n",
        "sns.heatmap(cm, annot=True, fmt=\"d\", cmap=\"Blues\", xticklabels=[\"neg\",\"pos\"], yticklabels=[\"neg\",\"pos\"])\n",
        "plt.title(\"Confusion Matrix\")\n",
        "plt.show()\n",
        "\n",
    
        "val_df = pd.DataFrame({\"text\": X_val, \"true\": y_val, \"pred\": y_pred})\n",
        "misclassified = val_df[val_df[\"true\"] != val_df[\"pred\"]].sample(10, random_state=42)\n",
        "print(\"\\n🔹 Misclassified Examples:\")\n",
        "for i, row in misclassified.iterrows():\n",
        "    print(f\"Tweet: {row['text']}\")\n",
        "    print(f\"True : {row['true']} | Pred : {row['pred']}\\n\")\n"
      ]
