        "import nltk\n",
        "from nltk.corpus import stopwords\n",
        "from nltk.tokenize import word_tokenize\n",
        "import string\n",
        "\n",
        "# تحميل الموارد\n",
        "nltk.download(\"punkt\")\n",
        "nltk.download(\"punkt_tab\")   
        "nltk.download(\"stopwords\")\n",
        "\n",
        "stop_words = set(stopwords.words(\"english\"))\n",
        "\n",
        "def preprocess_tokens(tokens):\n",
        "    processed = []\n",
        "    for tok in tokens:\n",
        "        if tok in stop_words:         # remove stopwords\n",
        "            continue\n",
        "        if tok in string.punctuation: # remove punctuation\n",
        "            continue\n",
        "        if tok.isdigit():             # replace numbers\n",
        "            processed.append(\"<NUM>\")\n",
        "        else:\n",
        "            processed.append(tok)\n",
        "    return processed\n",
        "\n",
        "# مثال\n",
        "sample = \"I bought 2 books, and they were amazing!\"\n",
        "tokens = word_tokenize(sample.lower())\n",
        "print(\"Before:\", tokens)\n",
        "print(\"After :\", preprocess_tokens(tokens))\n"
      ]
