        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "from collections import Counter\n",
        "import re\n",
        "import emoji\n",
        "# --------- 1. Class Distribution ---------\n",
        "plt.figure(figsize=(6,4))\n",
        "sns.countplot(x=\"target\", data=train_df, order=train_df[\"target\"].value_counts().index)\n",
        "plt.title(\"Class Distribution (Training Set)\")\n",
        "plt.xlabel(\"Sentiment\")\n",
        "plt.ylabel(\"Count\")\n",
        "plt.show()\n",
        "\n",
        "# --------- 2. Tweet Length Histogram ---------\n",
        "train_df[\"tweet_length\"] = train_df[\"text\"].apply(len)\n",
        "\n",
        "plt.figure(figsize=(6,4))\n",
        "sns.histplot(train_df[\"tweet_length\"], bins=50, kde=False)\n",
        "plt.title(\"Tweet Length Distribution\")\n",
        "plt.xlabel(\"Tweet Length (characters)\")\n",
        "plt.ylabel(\"Frequency\")\n",
        "plt.show()\n",
        "\n",
        "# --------- 3. Top Frequent Tokens ---------\n",
        "def clean_and_tokenize(text):\n",
        "    text = text.lower()\n",
